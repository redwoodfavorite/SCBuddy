{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "sc2.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "alarms",
    "tabs"
  ],
  "background": {
    "scripts": ["util.js", "node_modules/axios/dist/axios.min.js","background.js"],
    "persistent": false
  }
}
